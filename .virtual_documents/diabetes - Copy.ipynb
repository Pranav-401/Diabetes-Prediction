# Outlier Detection 
# IQR
# 50%
# 24.65-->25% + 50%
# 24.65-->25%
for feature in df :
    Q1 =df[feature].quantile(0.25)
    Q3 =df[feature].quantile(0.75)
    IQR = Q3 - Q1
    lower = Q1-1.5*IQR
    upper = Q3+1.5*IQR
    if df[(df[feature]>upper)].any(axis=None):
        print(feature, "yes")
    else:
        print(feature, "No")


